import random

def main():
    randomnumber = random.random()    #get a random number
    if (randomnumber < 0.8):          #80% chance that the random is less than 0.8
        favourite = "dogs"        
    elif (randomnumber < 0.9):        #10% chance that the number is between 0.8 and 0.9
        favourite = "cats"
    else:                             #10% chance that the number is not less than 0.9.
        favourite = "bats"
    print("I love " + favourite) 


main()
